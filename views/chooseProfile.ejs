<%- include('./partials/header') %>
<section class="choose-profile-content-container">
        <!--begin main-->
<!--content-->

<% families.forEach(function(family) { %>
        <% family.family_member.forEach(function(member) { %>
                <% profiles.forEach(function(profile) { %>
                        <% if (profile.id === member) { %>
                                <article class="chooseProfile">
                                        <img src="https://fdnd-agency.directus.app/assets/<%= profile.avatar %>" alt="profile-avatar">
                                <a href="/personal/<%= member %>"></a>
                                        <h2><%= profile.name %></h2>
                                </article>
                        <% } %>
                <% }) %>
        <% }) %>
<% }) %>

</section>

