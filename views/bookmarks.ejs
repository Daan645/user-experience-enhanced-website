<!-- bookmarks.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bookmarks</title>
</head>
<body>
<h1>Bookmarked Items</h1>
<ul>
    <% bookmarkedItems.forEach(function(item) { %>
        <li><%= item %></li>
    <% }); %>
</ul>
</body>
</html>